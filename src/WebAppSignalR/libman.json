  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "dist/jquery.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "clipboard@2.0.4",
